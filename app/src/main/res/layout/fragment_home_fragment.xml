<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home_fragment">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:background="#000">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/upper_bar"
            >

            <ImageView
                android:elevation="6dp"
                android:id="@+id/image_power1"
                android:layout_width="50dp"
                android:layout_height="48dp"
                android:paddingTop="10dp"
                android:src="@drawable/kiet_logo"/>
            <ImageView
                android:elevation="6dp"
                android:layout_width="50dp"
                android:layout_height="48dp"
                android:layout_toRightOf="@id/image_power1"
                android:paddingTop="10dp"
                android:id="@+id/i"
                android:src="@drawable/tbi"/>

            <ImageView

                android:layout_alignParentRight="true"
                android:layout_width="60dp"
                android:layout_height="48dp"
                android:elevation="6dp"
                android:id="@+id/ecell"
                android:paddingTop="10dp"
                android:src="@drawable/ecell"
                android:layout_marginRight="8dp" />



        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_below="@+id/upper_bar"
            android:id="@+id/text_"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:id="@+id/text_ecell"

                android:fontFamily="@font/montserrat_bold"
                android:gravity="center"
                android:textColor="#76b900"
                android:text="THE\nENTREPRENEURSHIP\nCELL"
                android:textSize="30sp"
                android:textStyle="normal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_below="@+id/text_ecell"
                android:layout_centerHorizontal="true"
                android:fontFamily="sans-serif"

                android:gravity="center"
                android:text="KIET GROUP OF INSTITUTIONS"
                android:textColor="#fff"
                android:textSize="15sp"
                android:textStyle="normal" />

        </RelativeLayout>



        <LinearLayout
            android:id="@+id/layout_bottom_menu"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:elevation="6dp"

            android:layout_centerInParent="true"
            android:layout_height="match_parent"
            android:gravity="center"
            >

            <LinearLayout
                android:paddingRight="16dp"
                android:paddingLeft="16dp"
                android:paddingTop="10dp"
                android:layout_marginTop="24dp"
                android:gravity="center"

                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        android:id="@+id/layout_events"
                        android:orientation="vertical"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:elevation="6dp"
                        android:background="@drawable/bg"
                        android:layout_width="match_parent"
                        android:layout_height="120dp">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"

                            >
                            <ImageView
                                android:layout_centerHorizontal="true"
                                android:id="@+id/events_img"
                                android:layout_width="68dp"
                                android:layout_height="80dp"
                                android:src="@drawable/event"

                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="E-Summit"
                                android:gravity="center"
                                android:layout_centerHorizontal="true"
                                android:textSize="17sp"
                                android:textColor="#000"
                                android:layout_below="@id/events_img"/>
                        </RelativeLayout>


                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="5dp"
                        android:id="@+id/layout_shedule"
                        android:orientation="vertical"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:elevation="6dp"
                        android:background="@drawable/bg"
                        android:layout_width="match_parent"
                        android:layout_height="120dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"

                            >

                            <ImageView
                                android:id="@+id/events_img1"
                                android:layout_width="60dp"
                                android:layout_height="80dp"
                                android:layout_centerHorizontal="true"
                                android:src="@drawable/event"

                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Hult Prize"
                                android:layout_centerHorizontal="true"
                                android:textSize="17sp"
                                android:gravity="center"
                                android:textColor="#000"
                                android:layout_below="@id/events_img1"/>
                        </RelativeLayout>
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        android:id="@+id/layout_speakers"
                        android:orientation="vertical"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:elevation="6dp"
                        android:background="@drawable/bg"
                        android:layout_width="match_parent"
                        android:layout_height="120dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"

                            >
                            <ImageView
                                android:layout_centerHorizontal="true"
                                android:id="@+id/events_img2"
                                android:layout_width="68dp"
                                android:layout_height="80dp"
                                android:src="@drawable/event"

                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="NEC"
                                android:layout_centerHorizontal="true"
                                android:textSize="17sp"
                                android:textColor="#000"
                                android:gravity="center"
                                android:layout_below="@id/events_img2"/>

                        </RelativeLayout>

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:paddingBottom="10dp"
                android:paddingTop="5dp"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="16dp"
                android:gravity="center"

                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        android:id="@+id/layout_sponsors"
                        android:orientation="vertical"
                        android:layout_marginLeft="10dp"
                        android:elevation="6dp"
                        android:layout_marginRight="10dp"
                        android:background="@drawable/bg"
                        android:layout_width="match_parent"
                        android:layout_height="120dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"

                            >
                            <ImageView
                                android:layout_centerHorizontal="true"
                                android:id="@+id/events_img3"
                                android:layout_width="60dp"
                                android:layout_height="80dp"
                                android:src="@drawable/event"

                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Workshops"
                                android:layout_centerHorizontal="true"
                                android:textSize="17sp"
                                android:gravity="center"
                                android:textColor="#000"
                                android:layout_below="@id/events_img3"/>
                        </RelativeLayout>

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        android:orientation="vertical"
                        android:id="@+id/layout_ourteam"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:elevation="6dp"
                        android:background="@drawable/bg"
                        android:layout_width="match_parent"
                        android:layout_height="120dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"

                            >
                            <ImageView
                                android:layout_centerHorizontal="true"
                                android:id="@+id/events_img4"
                                android:layout_width="68dp"
                                android:layout_height="80dp"
                                android:src="@drawable/event"

                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="EAC"
                                android:gravity="center"
                                android:layout_centerHorizontal="true"
                                android:textSize="17sp"
                                android:textColor="#000"
                                android:layout_below="@id/events_img4"/>
                        </RelativeLayout>

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        android:orientation="vertical"
                        android:layout_marginLeft="10dp"
                        android:id="@+id/layout_faq"
                        android:elevation="6dp"
                        android:layout_marginRight="10dp"
                        android:background="@drawable/bg"
                        android:layout_width="match_parent"
                        android:layout_height="120dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"


                            >
                            <ImageView
                                android:layout_centerHorizontal="true"
                                android:id="@+id/events_img5"
                                android:layout_width="68dp"
                                android:layout_height="80dp"
                                android:src="@drawable/event"

                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Innompic Games"
                                android:layout_centerHorizontal="true"
                                android:gravity="center"
                                android:textSize="17sp"
                                android:textColor="#000"

                                android:layout_below="@id/events_img5"
                                />
                        </RelativeLayout>

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

    </RelativeLayout>


</FrameLayout>